{
    "name": "webpieces-ts",
    "version": "1.0.0",
    "packageManager": "npm@10.8.1",
    "workspaces": [
        "packages/server",
        "packages/client",
        "packages/rules",
        "packages/core/*",
        "packages/http/*",
        "packages/tooling/*",
        "apps/*"
    ],
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "build": "nx build example-app",
        "build-all": "npm run build-all:compile && npm run build-all:lint && npm run build-all:test && npm run build-all:circular-deps && npm run build-all:architecture",
        "build-all:compile": "nx run-many --target=build --all",
        "build-all:lint": "nx run-many --target=lint --all",
        "build-all:test": "nx run-many --target=test --all",
        "build-all:circular-deps": "nx run-many --target=check-circular-deps --all",
        "build-all:architecture": "nx run architecture:validate-no-cycles && nx run architecture:validate-architecture-unchanged",
        "clean": "rm -rf dist coverage .nx",
        "lint": "eslint . --ext .ts,.tsx,.js,.jsx --quiet",
        "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix --quiet",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "setup:npm-publishers": "ts-node scripts/setup-npm-trusted-publishers.ts"
    },
    "repository": {
        "type": "git",
        "url": "personal:deanhiller/webpieces-ts.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "devDependencies": {
        "@nx/eslint": "^22.1.3",
        "@nx/jest": "^22.0.2",
        "@nx/js": "^22.0.2",
        "@playwright/test": "^1.57.0",
        "@swc-node/register": "~1.9.1",
        "@swc/core": "~1.5.7",
        "@swc/helpers": "~0.5.11",
        "@types/eslint": "^9.6.1",
        "@types/express": "^5.0.5",
        "@types/jest": "^30.0.0",
        "@types/minimatch": "^5.1.0",
        "@types/node": "^24.9.2",
        "@typescript-eslint/eslint-plugin": "^8.48.0",
        "@typescript-eslint/parser": "^8.48.0",
        "@webpieces/dev-config": "workspace:*",
        "eslint": "^9.39.1",
        "jest": "^30.2.0",
        "madge": "^8.0.0",
        "nx": "^22.0.2",
        "prettier": "^3.6.2",
        "ts-jest": "^29.4.5",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tslib": "^2.3.0",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@inversifyjs/binding-decorators": "^1.1.5",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "express": "^5.1.0",
        "inversify": "^7.10.4",
        "minimatch": "^9.0.0",
        "reflect-metadata": "^0.2.2"
    }
}
