{
    "name": "webpieces-ts",
    "version": "1.0.0",
    "packageManager": "npm@10.8.1",
    "workspaces": [
        "packages/server",
        "packages/client",
        "packages/rules",
        "packages/core/*",
        "packages/http/*",
        "packages/tooling/*",
        "libraries/apis/*",
        "apps/*"
    ],
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "build": "nx build example-server && nx build example-client",
        "start:server": "./scripts/local-start.sh server",
        "start:client": "./scripts/local-start.sh client",
        "stop:server": "./scripts/local-start.sh stop-server",
        "stop:client": "./scripts/local-start.sh stop-client",
        "build-validate-versions": "node scripts/build-validate-versions.js",
        "preinstall": "node scripts/build-validate-versions.js",
        "build-all": "npm run build-validate-versions && npm run build-all:compile && npm run build-all:validate-eslint && npm run build-all:lint && npm run build-all:test && npm run arch:validate-complete",
        "build-all:validate-eslint": "nx run dev-config:validate-eslint-sync",
        "build-all:compile": "nx run-many --target=build --all",
        "build-all:lint": "nx run-many --target=lint --all",
        "build-all:test": "nx run-many --target=test --all",
        "arch:generate": "nx run architecture:generate",
        "arch:visualize": "nx run architecture:visualize",
        "arch:validate": "nx run architecture:validate-no-cycles && nx run architecture:validate-no-skiplevel-deps",
        "arch:validate-all": "nx run architecture:validate-no-cycles && nx run architecture:validate-no-skiplevel-deps && nx run architecture:validate-architecture-unchanged",
        "arch:check-circular": "nx run-many --target=check-circular-deps --all",
        "arch:check-circular-affected": "nx affected --target=check-circular-deps",
        "arch:validate-complete": "npm run arch:validate-all && npm run arch:check-circular",
        "clean": "rm -rf dist coverage .nx",
        "lint": "eslint . --ext .ts,.tsx,.js,.jsx --quiet",
        "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix --quiet",
        "format": "prettier --write .",
        "format:check": "prettier --check ."
    },
    "repository": {
        "type": "git",
        "url": "personal:deanhiller/webpieces-ts.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "devDependencies": {
        "@angular-devkit/build-angular": "^20.3.0",
        "@angular/animations": "^20.3.0",
        "@angular/build": "^20.3.0",
        "@angular/cli": "^20.3.0",
        "@angular/common": "^20.3.0",
        "@angular/compiler": "^20.3.0",
        "@angular/compiler-cli": "^20.3.0",
        "@angular/core": "^20.3.0",
        "@angular/forms": "^20.3.0",
        "@angular/platform-browser": "^20.3.0",
        "@angular/platform-browser-dynamic": "^20.3.0",
        "@angular/router": "^20.3.0",
        "@nx/eslint": "^22.1.3",
        "@nx/eslint-plugin": "^22.2.1",
        "@nx/jest": "^22.0.2",
        "@nx/js": "^22.0.2",
        "@playwright/test": "^1.57.0",
        "@swc-node/register": "~1.9.1",
        "@swc/core": "~1.5.7",
        "@swc/helpers": "~0.5.11",
        "@types/eslint": "^9.6.1",
        "@types/express": "^5.0.5",
        "@types/jest": "^29.5.14",
        "@types/minimatch": "^5.1.0",
        "@types/node": "^24.9.2",
        "@typescript-eslint/eslint-plugin": "^8.48.0",
        "@typescript-eslint/parser": "^8.48.0",
        "@webpieces/dev-config": "workspace:*",
        "eslint": "^9.39.1",
        "jest": "^29.7.0",
        "madge": "^8.0.0",
        "nx": "^22.0.2",
        "prettier": "^3.6.2",
        "rxjs": "^7.8.1",
        "ts-jest": "^29.4.5",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "zone.js": "^0.15.0"
    },
    "dependencies": {
        "@inversifyjs/binding-decorators": "^1.1.5",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "express": "^5.1.0",
        "inversify": "^7.10.4",
        "minimatch": "^10.0.1",
        "reflect-metadata": "^0.2.2"
    }
}
